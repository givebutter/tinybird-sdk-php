{
    "name": "brd6/tinybird-sdk-php-batch-queries-example",
    "description": "Batch Queries example for Tinybird SDK PHP",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "brd6/tinybird-sdk-php": "@dev",
        "vlucas/phpdotenv": "^5.6",
        "nyholm/psr7": "^1.8",
        "symfony/http-client": "^6.4|^7.0",
        "guzzlehttp/promises": "^2.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../",
            "options": {
                "symlink": true
            }
        }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}

